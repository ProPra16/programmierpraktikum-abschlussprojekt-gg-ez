<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" fillWidth="false" prefHeight="150.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane fx:id="grid" alignment="CENTER" hgap="10.0" vgap="10.0">
            <children>
                <Label text="Time">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font></Label>
                <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onButton" prefWidth="40.0" text="On" />
                        <Button mnemonicParsing="false" onAction="#offButton" prefWidth="40.0" text="Off" />
                    </children>
                </HBox>
                <Label text="Babysteps" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font></Label>
            </children>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </children>
</VBox>
